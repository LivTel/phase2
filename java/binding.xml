 	</mapping>
 	
 	<mapping name="x-sky-brightness-constraint" class="ngat.phase2.XSkyBrightnessConstraint" extends="ngat.phase2.IObservingConstraint">
		<value name="sky-brightness-category" field="skyBrightnessCategory"			usage="optional"/>
	</mapping>
 	

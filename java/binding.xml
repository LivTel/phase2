 	<mapping class="ngat.phase2.IObservingPreference" abstract="true"/>
 	<mapping class="ngat.phase2.IProgram" abstract="true"/>
 	<mapping class="ngat.phase2.IProposal" abstract="true"/>
 	<mapping class="ngat.phase2.IPublishedSystemProperties" abstract="true"/>
 	<mapping class="ngat.phase2.IRevision" abstract="true"/>

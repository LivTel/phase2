	<!-- *************************** OBSERVATION SEQUENCE COMPONENTS *************************** -->

	<mapping class="ngat.phase2.ISequenceComponent" abstract="true"/>
	
		<mapping name="x-branch-component" class="ngat.phase2.XBranchComponent" extends="ngat.phase2.ISequenceComponent">
	 		<value name="component-name" field="componentName" usage="optional"/>
	 		<collection field="sequence" 
				factory="org.jibx.runtime.Utility.arrayListFactory" 
				get-method="listChildComponents" 
				set-method="setSequence"/>
	 	</mapping>
		
		<mapping name="x-executive-component" class="ngat.phase2.XExecutiveComponent" extends="ngat.phase2.ISequenceComponent">
			<value name="component-name" field="componentName" usage="optional"/>
	 		<structure field="action" map-as="ngat.phase2.IExecutiveAction" 			usage="optional"/>
	 	</mapping>
 	
 			<mapping class="ngat.phase2.IExecutiveAction" abstract="true"/>

			<mapping name="i-acquisition-config" class="ngat.phase2.IAcquisitionConfig" 
				extends="ngat.phase2.IExecutiveAction" 
				factory="ngat.phase2.util.Factory.acquisitionConfigFactory" />
			
				<mapping name="x-acquisition-config" class="ngat.phase2.XAcquisitionConfig" extends="ngat.phase2.IAcquisitionConfig">
			 		<structure map-as="ngat.phase2.IAcquisitionConfig"								usage="optional"/>
					<value name="mode" 					 		field="mode"						usage="optional"/>
					<value name="target-instrument-name" 		field="targetInstrumentName"		usage="optional"/>
					<value name="acquisition-instrument-name" 	field="acquisitionInstrumentName"	usage="optional"/>
					<value name="allow-alternative" 			field="allowAlternative"			usage="optional"/>
					<value name="precision" 			field="precision"			usage="optional"/>
				</mapping>

			<mapping name="i-aperture-config" class="ngat.phase2.IApertureConfig" 
				extends="ngat.phase2.IExecutiveAction" 
				factory="ngat.phase2.util.Factory.apertureConfigFactory" />
				
				<mapping name="x-aperture-config" class="ngat.phase2.XApertureConfig" extends="ngat.phase2.IApertureConfig">
			 		<structure map-as="ngat.phase2.IApertureConfig"							usage="optional"/>
				</mapping>

			<mapping name="i-autoguider-config" class="ngat.phase2.IAutoguiderConfig" 
				extends="ngat.phase2.IExecutiveAction" 
				factory="ngat.phase2.util.Factory.autoguiderConfigFactory" />
				
				<mapping name="x-autoguider-config" class="ngat.phase2.XAutoguiderConfig" extends="ngat.phase2.IAutoguiderConfig" >
					<structure map-as="ngat.phase2.IAutoguiderConfig"							usage="optional"/>
			    	<value name="autoguider-mode" 		field="autoguiderMode" 					usage="optional"/>
					<value name="autoguider-name" 		field="autoguiderName" 					usage="optional"/>
			 	</mapping>

			<mapping name="i-beamsteering-config" class="ngat.phase2.IBeamSteeringConfig" 
				extends="ngat.phase2.IExecutiveAction" 
				factory="ngat.phase2.util.Factory.beamsteeringConfigFactory" />
				
				<mapping name="x-beamsteering-config" class="ngat.phase2.XBeamSteeringConfig" extends="ngat.phase2.IBeamSteeringConfig">
					<structure map-as="ngat.phase2.IBeamSteeringConfig"							usage="optional"/>
					<structure field="upperSlideConfig" map-as="ngat.phase2.XOpticalSlideConfig" 	usage="optional"/>
					<structure field="lowerSlideConfig" map-as="ngat.phase2.XOpticalSlideConfig" 	usage="optional"/>
				</mapping>
			
			<mapping name="i-opticalslide-config" class="ngat.phase2.IOpticalSlideConfig" 

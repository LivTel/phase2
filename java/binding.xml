 	<mapping class="ngat.phase2.ITelescopeConfig" abstract="true"/>
 	<mapping class="ngat.phase2.ITimePeriod" abstract="true"/>
 	<mapping class="ngat.phase2.ITimingConstraint" abstract="true"/>
 	<mapping class="ngat.phase2.ITransaction" abstract="true"/>
 	<mapping class="ngat.phase2.IUser" abstract="true"/>
 	<mapping class="ngat.phase2.IVersion" abstract="true"/>
	
	<!-- implementations -->
 	
 	<mapping name="x-access-permission" class="ngat.phase2.XAccessPermission" extends="ngat.phase2.IAccessPermission">
 		<structure map-as="ngat.phase2.XPhase2Identity" 							usage="optional"/>
 		<value name="user-id"				field="userID" 							usage="optional"/>
		<value name="proposal-id"			field="proposalID" 						usage="optional"/>
		<value name="role"					field="role" 							usage="optional"/>
 	</mapping>
	
	<mapping name="x-account" class="ngat.phase2.XAccount" extends="ngat.phase2.IAccount">
		<structure map-as="ngat.phase2.XPhase2Identity" 							usage="optional"/>
		<!-- <value name="type"					field="type" 							usage="optional"/> -->
		<!-- <value name="name"					field="name" 							usage="optional"/> -->
		<!-- <value name="description"			field="description" 					usage="optional"/> -->
		<!-- <value name="originator"			field="originator" 						usage="optional"/> -->
		<!-- <value name="start-time"			field="startTime" 						usage="optional"/> -->
		<!-- <value name="end-time"				field="endTime" 						usage="optional"/> -->
		<value name="allocated"				field="allocated" 						usage="optional"/>
		<value name="consumed"				field="consumed" 						usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-airmass-constraint" class="ngat.phase2.XAirmassConstraint" extends="ngat.phase2.IObservingConstraint">
		<value name="maximum-airmass" 		field="maximumAirmass"				usage="optional"/>
	</mapping>


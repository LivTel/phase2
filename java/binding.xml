 		<value name="is-overlap"			field="isOverlap"						usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-simple-observing-preference" class="ngat.phase2.XSimpleObservingPreference" extends="ngat.phase2.IObservingPreference">

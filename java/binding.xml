<?xml version="1.0" encoding="UTF-8"?>

<!-- JIBX BINDING DEFINITION FOR ALL PHASE 2 OBJECTS AND TRANSPORT WRAPPERS -->

<binding forwards="false" value-style="attribute">
	
	<!-- *************************** PHASE2, ACCESS AND HISTORY MODEL COMPONENTS *************************** -->
	
	<!-- abstract / interfaces -->
	
	<mapping class="ngat.phase2.IPhase2" abstract="true"/>
 	<mapping class="ngat.phase2.IPhase2Identity" abstract="true"/>
 	
	<mapping class="ngat.phase2.IAccessPermission" abstract="true"/>
	<mapping class="ngat.phase2.IAccount" abstract="true"/>

 	<mapping name="x-ephemeris-timing-constraint" class="ngat.phase2.XEphemerisTimingConstraint" extends="ngat.phase2.ITimingConstraint">
 		<value name="start" 				field="start" 							usage="optional"/>
		<value name="end" 					field="end" 							usage="optional"/>
		<value name="cycle-period" 			field="cyclePeriod" 					usage="optional"/>
		<value name="window" 				field="window" 							usage="optional"/>
		<value name="phase" 				field="phase" 							usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-ephemeris-track-node" class="ngat.phase2.XEphemerisTrackNode">
    	<value name="time" 					field="time"							usage="optional"/>
		<value name="ra" 					field="ra" 								usage="optional"/>
		<value name="dec" 					field="dec" 							usage="optional"/>
		<value name="ra-dot" 				field="raDot" 							usage="optional"/>
		<value name="dec-dot" 				field="decDot" 							usage="optional"/>
 	</mapping>

	<mapping name="x-filter-def" class="ngat.phase2.XFilterDef" > 
		<value name="filter-name" field="filterName" usage="optional"/> 
	</mapping>
	
	<mapping name="x-filter-spec" class="ngat.phase2.XFilterSpec" extends="ngat.phase2.IFilterSpec">
		<collection field="filterList" 
			factory="org.jibx.runtime.Utility.arrayListFactory"
			get-method="getFilterList" 
			set-method="setFilterList" usage="optional"/>
	</mapping>
	
 	<mapping name="x-fixed-timing-constraint" class="ngat.phase2.XFixedTimingConstraint" extends="ngat.phase2.ITimingConstraint">
 		<value name="fixed-time" 			field="fixedTime" 						usage="optional"/>
		<value name="slack" 				field="slack" 							usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-flexible-timing-constraint" class="ngat.phase2.XFlexibleTimingConstraint" extends="ngat.phase2.ITimingConstraint">
 		<value name="activation-date" 		field="activationDate" 					usage="optional"/>
		<value name="expiry-date" 			field="expiryDate" 						usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-group" class="ngat.phase2.XGroup" extends="ngat.phase2.IGroup">
 		<structure map-as="ngat.phase2.XPhase2Identity" 									usage="optional"/>
 		<structure field="timingConstraint" map-as="ngat.phase2.ITimingConstraint" 			usage="optional"/>
		<collection field="observingConstraints" 
			factory="org.jibx.runtime.Utility.arrayListFactory" 
			get-method="listObservingConstraints" 
			set-method="setObservingConstraints"/>
		<!-- 
		<collection field="observingPreferences" 
			factory="org.jibx.runtime.Utility.arrayListFactory" 
			get-method="listObservingPreferences" 
			set-method="setObservingPreferences" usage="optional"/> 
		-->
		<value name="priority" field="priority" usage="optional"/>
		<value name="active" field="active" usage="optional"/>
		<value name="urgent" field="urgent" usage="optional"/>
 	</mapping>
   	
   	<mapping name="x-history-item" class="ngat.phase2.XHistoryItem" extends="ngat.phase2.IHistoryItem">
 		<structure map-as="ngat.phase2.XPhase2Identity" usage="optional"/>
 		<value name="scheduled-time"		field="scheduledTime" usage="optional"/>
 		<value name="completion-time"		field="completionTime" usage="optional"/>
 		<value name="completion-status"		field="completionStatus" usage="optional"/>
 		<value name="error-code"			field="errorCode" usage="optional"/>
 		<value name="error-message"			field="errorMessage" usage="optional"/>
		<!-- qosStatistics, JiBX doesn't support sets -->
 	</mapping>
 	
	<mapping name="x-hour-angle-constraint" class="ngat.phase2.XHourAngleConstraint" extends="ngat.phase2.IObservingConstraint">
		<value name="minimum-hour-angle" 		field="minimumHourAngle"				usage="optional"/>
 		<value name="maximum-hour-angle" 		field="maximumHourAngle"				usage="optional"/>
	</mapping>
	
	<mapping name="x-lock" class="ngat.phase2.XLock">
		<value name="id" 					field="id" 								usage="optional"/>
		<value name="key" 					field="key" 							usage="optional"/>
		<value name="client-ref" 			field="clientRef" 						usage="optional"/>
		<value name="date" 					field="date" 							usage="optional"/>
 	</mapping>
 	
 	<mapping name="x-login-record" class="ngat.phase2.XLoginRecord" extends="ngat.phase2.ILoginRecord">
		<structure map-as="ngat.phase2.XPhase2Identity" 							usage="optional"/>
		<value name="user-id" 				field="userId" 							usage="optional"/>
		<value name="java-version" 			field="javaVersion" 					usage="optional"/>
		<value name="os-arch" 				field="osArch" 							usage="optional"/>
		<value name="os-name" 				field="osName" 							usage="optional"/>
		<value name="os-version" 			field="osVersion" 						usage="optional"/>
		<value name="screen-size" 			field="screenSize" 						usage="optional"/>
 	</mapping>
 	
